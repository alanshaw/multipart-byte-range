{
  "name": "multipart-byte-range",
  "version": "2.0.2",
  "description": "Encode and decode multipart/byteranges stream.",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./decoder": {
      "import": "./src/decoder.js",
      "types": "./dist/src/decoder.d.ts"
    },
    "./encoder": {
      "import": "./src/encoder.js",
      "types": "./dist/src/encoder.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "test": "entail",
    "lint": "standard",
    "coverage": "c8 --reporter html --reporter text npm test"
  },
  "keywords": [
    "range",
    "multipart",
    "byteranges",
    "multipart/byteranges",
    "bytes",
    "header",
    "http",
    "parse",
    "parser",
    "encode",
    "decode"
  ],
  "author": "Alan Shaw",
  "license": "Apache-2.0 OR MIT",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@httpland/range-parser": "^1.2.0",
    "c8": "^9.1.0",
    "entail": "^2.1.2",
    "standard": "^17.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "streamsearch-web": "^1.0.0",
    "uint8arraylist": "^2.4.8"
  },
  "standard": {
    "ignore": [
      "*.ts"
    ]
  }
}
